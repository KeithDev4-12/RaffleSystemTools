  object FDQuery2: TFDQuery
    Connection = FDConnSQLite
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      'select '
      'id,'
      'AccountNumber,'
      'Name,'
      'Area,'
      'Address,'
      'Status,'
      'ShuffleOrder,'
      'ConnectionStatus,'
      'Year,'
      'EntryMode,'
      'Gender,'
      'isPosted,'
      'RateCode'
      'from '
      'memberconsumers'
      'where '
      '(EntryMode = :EntryMode1 '
      'OR'
      'EntryMode = :EntryMode2 '
      'OR'
      'EntryMode = :EntryMode3 )'
      'and'
      #10'Status IN (1)'
      'and'
      '(CASE '
      '  WHEN :AArea = '#39'ALL'#39
      '  THEN Year = :AYear'
      '  ELSE Area = :AArea '
      ' END)'
      'and Year = :AYear'
      ''
      '-- Status number '
      '-- 0 = Not Active'
      '-- 1 = Active'
      '-- 2 = Drawn'
      '-- 3 = Disconnected But Can Still Attend'
      '-- 4 = Registered But Cannot be Included (Not Paid LatestBill)'
      '-- ConnectionStatus'
      '-- 1 = Active'
      '-- 2 = Disconnected'
