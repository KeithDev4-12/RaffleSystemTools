  object qryAccountQualifier: TFDQuery
    Connection = FDConnMYSQL
    SQL.Strings = (
      'SELECT'
      '  AccountNumber,'
      '  BillMonth,'
      '  IF(('
      '-- Group_Concat(BillMonth) LIKE '#39'%0324%'#39' OR '
      'GROUP_Concat(BillMonth) LIKE '#39'%0424%'#39'),'#39'LATEST'#39','#39'OLD'#39') As Status'
      'FROM'
      '('
      '  SELECT'
      '    AccountNumber,'
      '    BillMonth'
      '  FROM'
      '    ArLedger'
      '  WHERE'
      '    AccountNumber = :AAccountNumber'
      '  -- AND'
      '  -- (BillMonth = '#39'0324'#39' OR BillMonth = '#39'0424'#39')'
      '  -- (BillMonth = '#39'0820'#39' OR BillMonth = '#39'0920'#39')'
      '  GROUP BY'
      '    BillMonth'
      '  HAVING'
      '    (SUM(DEBIT) - SUM(CREDIT)) = 0'
      '  Order By'
      '    Entry'
      '  Desc LIMIT 2'
      ') A'
      'Group By'
      '  AccountNumber')
    Left = 144
    Top = 136
    ParamData = <
      item
        Name = 'AACCOUNTNUMBER'
        DataType = ftWideString
        ParamType = ptInput
        Value = '01160013'
      end>
