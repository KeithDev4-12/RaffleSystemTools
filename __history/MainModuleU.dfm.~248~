  object qryMaster: TFDQuery
    Connection = FDConnMYSQL
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      'SELECT '
      '  Code,'
      '  AccountNumber,'
      '  Name,'
      '  Area,'
      '  Address,'
      '  ConnCode,'
      '  ConsCode,'
      '  RateCode'
      'FROM '
      '  MASTER m'
      'WHERE '
      '  Book = :ABook'
      '  -- Area = :AArea'
      ' -- AND'
      '-- Book = '#39'116'#39)
    Left = 48
    Top = 80
    ParamData = <
      item
        Name = 'ABOOK'
        ParamType = ptInput
      end>
