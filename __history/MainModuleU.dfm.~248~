  object qryMCQualified: TFDQuery
    Connection = FDConnSQLite
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      'select '
      'id,'
      'AccountNumber,'
      'Name,'
      'Area,'
      'Address,'
      'Status,'
      'ShuffleOrder,'
      'ConnectionStatus,'
      'Year,'
      'EntryMode,'
      'Gender,'
      'isPosted,'
      'RateCode'
      'from '
      'memberconsumers'
      'where '
      '(EntryMode = :EntryMode1 '
      'OR'
      'EntryMode = :EntryMode2 '
      'OR'
      'EntryMode = :EntryMode3 )'
      'and'
      #10'Status IN (1)'
      'and'
      '(CASE '
      '  WHEN :AArea = '#39'ALL'#39
      '  THEN Year = :AYear'
      '  ELSE Area = :AArea '
      ' END)'
      'and Year = :AYear'
      ''
      '-- Status number '
      '-- 0 = Not Registered'
      '-- 1 = Registered'
      '-- 2 = Drawn'
      '-- 4 = Registered But Cannot be Included (Not Paid LatestBill)'
      '-- ConnectionStatus'
      '-- 1 = Active'
      '-- 2 = Disconnected'
      '-- 3 = Disco-Vacant')
    Left = 408
    Top = 176
    ParamData = <
      item
        Name = 'ENTRYMODE1'
        DataType = ftWideString
        ParamType = ptInput
        Value = ''
      end
      item
        Name = 'ENTRYMODE2'
        DataType = ftWideString
        ParamType = ptInput
        Value = ''
      end
      item
        Name = 'ENTRYMODE3'
        DataType = ftWideString
        ParamType = ptInput
        Value = 'PRE-REGISTRATION'
      end
      item
        Name = 'AAREA'
        DataType = ftWideString
        ParamType = ptInput
        Value = 'ALL'
      end
      item
        Name = 'AYEAR'
        DataType = ftWideString
        ParamType = ptInput
        Value = '2024'
      end>
    object qryMCQualifiedid: TFDAutoIncField
      FieldName = 'id'
