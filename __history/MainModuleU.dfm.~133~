  object qryMCQualified: TFDQuery
    Connection = FDConnSQLite
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      'select '
      'id,'
      'AccountNumber,'
      'Name,'
      'Area,'
      'Address,'
      'Status,'
      'ShuffleOrder,'
      'ConnectionStatus,'
      'Year,'
      'EntryMode,'
      'Gender,'
      'isPosted,'
      'RateCode'
      'from '
      'memberconsumers'
      'where Status = 1'
      'and'
      '(CASE '
      '  WHEN :AArea = '#39'ALL'#39
      '  THEN Status = 1'
      '  ELSE Area = :AArea '
      ' END)'
      'and Year = :AYear'
      ''
      '-- Status number '
      '-- 0 = Not Active'
      '-- 1 = Active'
      '-- 2 = Drawn'
      '-- ConnectionStatus'
      '-- 1 = Active'
      '-- 2 = Disconnected'
      '-- 3 = Disco-Vacant')
    Left = 408
    Top = 176
    ParamData = <
      item
        Name = 'AAREA'
        DataType = ftWideString
        ParamType = ptInput
        Value = 'ALL'
      end
      item
        Name = 'AYEAR'
        DataType = ftWideString
        ParamType = ptInput
        Value = '2023'
      end>
