  object qryUnPaidBills: TFDQuery
    Connection = FDConnMYSQL
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      'Select a.billmonth,(a.reading) Pres,'
      '       (a.kilowatthour) KWHR,'
      '       a.Debit, a.Credit,'
      '       (a.Document) ORNumber,'
      
        '       a.InterestAmount,a.InterestVat,a.GTSVat,a.OtherVat,a.EVat' +
        ','
      '       count(a.AccountNumber) numrec'
      'from ARLedger a'
      'where a.Code =:ACode'
      
        'and CONCAT(Substring(BillMonth,3,2),SubString(BillMonth,1,2)) <=' +
        ' :ABillMonthReverse'
      'Group by a.BillMonth, a.AccountNumber'
      'Having numrec = 1'
      'Order by a.Date')
    Left = 40
    Top = 144
    ParamData = <
      item
        Name = 'ACODE'
        DataType = ftWideString
        ParamType = ptInput
        Value = '1'
      end
      item
        Name = 'ABILLMONTHREVERSE'
        ParamType = ptInput
      end>
  end
