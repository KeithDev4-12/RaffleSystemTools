  object qryWinnerMemberConsumers: TFDQuery
    Connection = FDConnSQLite
    FetchOptions.AssignedValues = [evMode]
    FetchOptions.Mode = fmAll
    SQL.Strings = (
      'select '
      'id,'
      'Year,'
      'AccountNumber,'
      'Name,'
      'Area,'
      'Address,'
      'OrderBy,'
      'Gender'
      'from '
      'winners'
      'where '
      '(CASE '
      '  WHEN :AArea = '#39'ALL'#39
      '  THEN Year = :AYear'
      '  ELSE Area = :AArea '
      ' END)'
      'and'
      'Year = :AYear'
      ''
      '-- Status number '
      '-- 0 = Not Active'
      '-- 1 = Active'
      '-- 2 = Drawn'
      '-- 3 = Disconnected But Can Still Attend'
      '-- ConnectionStatus'
      '-- 1 = Active'
      '-- 2 = Disconnected'
      '-- 3 = Disco-Vacant')
    Left = 408
    Top = 240
    ParamData = <
      item
        Name = 'AAREA'
        DataType = ftWideString
        ParamType = ptInput
